zarConSolucion: esNegra\r\t\t\t\t\tyEstado: estado1\r\t\t\t\t\tyPartida: partida.\r            \r\r            celda alHacerClick: [ \r                self actualizarInfoPara: estado1 vidas: vLabel1 puntos: pLabel1 \r            ].\r            \r            celda position: (100 + (x * tamañoCelda)) @ (100 + (y * tamañoCelda)).\r            mainLayout addMorph: celda.\r            celdasMorphs add: celda.\r        ]\r    ].\r\r\r    anchoVentana := 600. \r    \r    (partida estadoJugadores size > 1) ifTrue: [\r        estado2 := partida estadoJugadores second.\r        anchoVentana := 1200. \r        \r\r\r        vLabel2 := Ventana new construirLabelConTexto: \'Vidas: \', estado2 vidas asString.\r        pLabel2 := Ventana new construirLabelConTexto: \'Puntos: \', estado2 puntaje asString.\r        \r        panelInfo2 := Morph new color: Color transparent; layoutPolicy: TableLayout new; listDirection: #leftToRight; cellInset: 20.\r        panelInfo2 addMorph: vLabel2; addMorph: pLabel2.\r        panelInfo2 position: (350 + 600) @ 10. \r        mainLayout addMorph: panelInfo2.\r        \r       \r        self dibujarPistasFilas: 100 tamaño: tamañoCelda enX: (10 + 600).\r        self dibujarPistasColumnas: (100 + 600) yGrilla: 100 tamaño: tamañoCelda.\r \r        1 to: dimension do: [ :y |\r            1 to: dimension do: [ :x |\r                | celda esNegra |\r                esNegra := partida tablero esNegraEnX: x y: y.\r                celda := CeldaMorph new layoutPolicy: nil; hResizing: #rigid; vResizing: #rigid; extent: tamañoCelda @ tamañoCelda.\r\t\t\t\t\tcelda setProperty: #punto toValue: (x@y).\r\t\t\t\t\t celda setProperty: #indiceJugador toValue: 2.                 \r\r                "celda inicializarConSolucion: esNegra yEstado: estado2."\r\t\t\t\t\tcelda\r\t\t\t\t\t\tinicializarConSolucion: esNegra\r\t\t\t\t\t\tyEstado: estado2\r\t\t\t\t\t\tyPartida: partida.                \r\r                celda alHacerClick: [ \r                    self actualizarInfoPara: estado2 vidas: vLabel2 puntos: pLabel2 \r                ].\r\r                celda position: ((100 + 600) + (x * tamañoCelda)) @ (100 + (y * tamañoCelda)).\r                mainLayout addMorph: celda.\r                celdasMorphs add: celda.\r            ]\r        ].\r    ].\r\r\r    window extent: anchoVentana @ 600.\r    window position: 50 @ 50.\r\r    \r\r    (partida estadoJugadores size > 1) \r        ifTrue: [ \r            \r            anchoVentana := 1300. \r        ]\r        ifFalse: [ \r            \r            anchoVentana := 650. \r        ].\r\r  \r    window extent: anchoVentana @ 700.\r\r    window position: (Display extent - window extent) // 2.',			#stamp : 'Anonymous 11/19/2025 21:44',			#package : #juego-UI		},		#newMethod : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #dibujarJuego,			#protocol : #drawing,			#sourceCode : 'dibujarJuego\r    | dimension tamañoCelda anchoVentana offsetJ2 estado1 estado2 vLabel1 pLabel1 vLabel2 pLabel2 panelInstrucciones panelInfo1 panelInfo2 |\r    \r    "1. Limpieza"\r    mainLayout delete.\r    mainLayout := Morph new color: (Color gray: 0.85).\r    mainLayout hResizing: #spaceFill; vResizing: #spaceFill.\r    window addMorph: mainLayout frame: (0@0 extent: 1@1).\r\r    dimension := partida tablero dimension.\r    \r    dimension = 5 ifTrue: [ tamañoCelda := 30 ].\r    dimension = 10 ifTrue: [ tamañoCelda := 25 ].\r    dimension = 15 ifTrue: [ tamañoCelda := 20 ].\r\r    offsetJ2 := 650.\r\r    "========================================================"\r    " JUGADOR 1 (Izquierda) "\r    "========================================================"\r    estado1 := partida estadoJugadores first.\r\r    "--- Puntos J1 ---"\r\r    panelInfo1 := Morph new color: Color transparent; layoutPolicy: TableLayout new; listDirection: #leftToRight; cellInset: 20.\r    vLabel1 := Ventana new construirLabelConTexto