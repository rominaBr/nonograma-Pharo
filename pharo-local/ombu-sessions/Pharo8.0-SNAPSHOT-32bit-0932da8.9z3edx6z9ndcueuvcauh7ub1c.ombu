rucciones addMorph: labelInst2.\r   \r   panelInstrucciones position: 350 @ 50.\r   panelInstrucciones width: 240.\r\tpanelInstrucciones height: 60.\r   mainLayout addMorph: panelInstrucciones.\r\t\r\t\t\r   dimension = 5 ifTrue: [ tamañoCelda := 30 ].\r   dimension = 10 ifTrue: [ tamañoCelda := 25 ].\r   dimension = 15 ifTrue: [ tamañoCelda := 20 ].\r\r\r   xGrilla := 100. \r   yGrilla := 100. \r\r\tself dibujarPistasFilas: yGrilla tamaño: tamañoCelda.\r   self dibujarPistasColumnas: xGrilla yGrilla: yGrilla tamaño: tamañoCelda.\r\r\testadoDelJugador := partida estadoJugadores first.\t\r\t\r   1 to: dimension do: [ :y |\r        1 to: dimension do: [ :x |\r            | celdaMorph esNegra |\r\r\t\t\t\tesNegra:= partida tablero esNegraEnX: x y: y.\r\t\t\t\tceldaMorph := CeldaMorph new.\r\t\t\t\t\r\t\t\t\tceldaMorph layoutPolicy: nil.\r\t\t\t\t\r\t\t\t\tceldaMorph extent: tamañoCelda @ tamañoCelda.\r\t\t\t\tceldaMorph setProperty: #punto toValue: (x@y).\t\t\t\t\r\t\t\t\t\r\t\t\t\tceldaMorph\r\t\t\t\t\tinicializarConSolucion: esNegra\r\t\t\t\t\tyEstado: estadoDelJugador.\r            \r\t\t\t\tceldaMorph alHacerClick: [ self actualizarInformacion ].\r            celdaMorph position: (xGrilla + (x * tamañoCelda)) @ (yGrilla + (y * tamañoCelda)).\r            \r            mainLayout addMorph: celdaMorph.\r            celdasMorphs add: celdaMorph.\r        ]\r    ].\t\r\r',			#stamp : 'Anonymous 11/19/2025 01:02',			#package : #juego-UI		},		#newMethod : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #dibujarJuego,			#protocol : #drawing,			#sourceCode : 'dibujarJuego\r    | anchoVentana |\r    \r    "Recorremos los jugadores (1 o 2)"\r    partida estadoJugadores doWithIndex: [ :estadoDelJugador :index |\r        | offsetX xGrilla yGrilla dimension tamañoCelda panelInfo panelInstrucciones labelVidasLocal labelPuntajeLocal |\r\r        "Si es J1, offset es 0. Si es J2, es 600"\r        offsetX := (index - 1) * 600.\r        dimension := partida tablero dimension.\r\r        "--- TU CÓDIGO DEL PANEL INFO (Adaptado con Offset) ---"\r        panelInfo := Morph new color: Color transparent; layoutPolicy: TableLayout new; listDirection: #leftToRight; cellInset: 20; wrapCentering: #left; listCentering: #center.\r        \r        labelVidasLocal := Ventana new construirLabelConTexto: \'Vidas: \', estadoDelJugador vidas asString.\r        labelPuntajeLocal := Ventana new construirLabelConTexto: \'Puntos: \', estadoDelJugador puntaje asString.\r        \r        panelInfo addMorph: labelVidasLocal; addMorph: labelPuntajeLocal.\r        \r        "Posición: 350 + Offset"\r        panelInfo position: (350 + offsetX) @ 10.\r        panelInfo width: 240; height: 30.\r        mainLayout addMorph: panelInfo.\r\r        "--- TU CÓDIGO DE INSTRUCCIONES (Adaptado con Offset) ---"\r        panelInstrucciones := Morph new color: Color transparent; layoutPolicy: TableLayout new; listDirection: #topToBottom; listCentering: #right; wrapCentering: #center.\r        panelInstrucciones addMorph: (Ventana new construirLabelConTexto: \'Clic Izq: Pintar\').\r        panelInstrucciones addMorph: (Ventana new construirLabelConTexto: \'Clic Der: X\').\r        \r        "Posición: 350 + Offset"\r        panelInstrucciones position: (350 + offsetX) @ 50.\r        panelInstrucciones width: 240; height: 60.\r        mainLayout addMorph: panelInstrucciones.\r        \r        "--- CONFIGURACIÓN ---"\r        dimension = 5 ifTrue: [ tamañoCelda := 30 ].\r        dimension = 10 ifTrue: [ tamañoCelda := 25 ].\r        dimension = 15 ifTrue: [ tamañoCelda := 20 ].\r\r        "Posición Grilla: 100 + Offset"\r        xGrilla := 100 + offsetX. \r        yGrilla := 100. \r\r        "Pistas con Offset (X = 10 + Offset)"\r        self dibujarPistasFilas: yGrilla tamaño: tamañoCelda enX: (10 + offsetX).\r        self dibujarPistasColumnas: xGrilla yGrilla: yGrilla tamaño: tamañoCelda.\r\r        "--- GRILLA ---"\r        1 to: dimension do: [ :y |\r            1 to: dimension do: [ :x |\r                | celdaMorph esNegra |\r                esNegra := partida tablero esNegraEnX: x y: y.\r                \r                celdaMorph := CeldaMorph new.\r                celdaMorph layoutPolicy: nil; hResizing: #rigid; vResizing: #rigid.\r                celdaMorph extent: tamañoCelda @ tamañoCelda.\r                celdaMorph setProperty: #punto toValue: (x@y).                \r                \r                celdaMorph\r                    inicializarConSolucion: esNegra\r                    yEstado: estadoDelJugador.\r                \r                "CONEXIÓN: Usamos el nuevo método y le pasamos las etiquetas locales"\r                celdaMorph alHacerClick: [ \r                    self actualizarInfoPara: estadoDelJugador etiquetas: {labelVidasLocal . labelPuntajeLocal} \r                ].\r                \r                celdaMorph position: (xGrilla + (x * tamañoCelda)) @ (yGrilla + (y * tamañoCelda)).\r                mainLayout addMorph: celdaMorph.\r                celdasMorphs add: celdaMorph.\r            ]\r        ].\r    ].\r\r    "Ajustamos el ancho de la ventana al final"\r    anchoVentana := (partida estadoJugadores size * 600).\r    window extent: anchoVentana @ 600.\r    window position: 50 @ 50.',			#stamp : 'Anonymous 11/19/2025 01:48',			#package : #juego-UI		}	}}OmEntry {	#tags : {		#author : 'Unknown',		#time : DateAndTime [ '2025-11-19T01:50:12.851728-03:00' ],		#prior : OmReference [ '3' ],		#self : OmReference [ '4' ]	},	#content : EpMethodModification {		#oldMethod : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #dibujarJuego,			#protocol : #drawing,			#sourceCode : 'dibujarJuego\r    | anchoVentana |\r    \r    "Recorremos los jugadores (1 o 2)"\r    partida estadoJugadores doWithIndex: [ :estadoDelJugador :ind