\r   panelInstrucciones color: Color transparent.\r   panelInstrucciones layoutPolicy: TableLayout new.\r\r   panelInstrucciones listDirection: #topToBottom. \r   panelInstrucciones listCentering: #right. \r   panelInstrucciones wrapCentering: #center.\r    \r\r   labelInst1 := Ventana new construirLabelConTexto: \'Clic Izq: Pintar\'.\r   labelInst2 := Ventana new construirLabelConTexto: \'Clic Der: Marcar X\'.    \r\r   panelInstrucciones addMorph: labelInst1.\r   panelInstrucciones addMorph: labelInst2.   \r\r   panelInstrucciones position: 350 @ 50.\r   panelInstrucciones width: 240.\r\tpanelInstrucciones height: 60.\r\r   mainLayout addMorph: panelInstrucciones.\r\r   dimension = 5 ifTrue: [ tamañoCelda := 30 ].\r   dimension = 10 ifTrue: [ tamañoCelda := 25 ].\r   dimension = 15 ifTrue: [ tamañoCelda := 20 ].\r\r   xGrilla := 100. \r   yGrilla := 100. \r\r\r\tself dibujarPistasFilas: yGrilla tamaño: tamañoCelda.\r   self dibujarPistasColumnas: xGrilla yGrilla: yGrilla tamaño: tamañoCelda.\r\r\r\testadoDelJugador := partida estadoJugadores first.\r\r   1 to: dimension do: [ :y |\r\r        1 to: dimension do: [ :x |\r\r            | celdaMorph esNegra |\r\r\t\t\t\tesNegra:= partida tablero esNegraEnX: x y: y.\r\t\t\t\tceldaMorph := CeldaMorph new.\r\r\t\t\t\tceldaMorph layoutPolicy: nil.\r\r\t\t\t\tceldaMorph extent: tamañoCelda @ tamañoCelda.\r\t\t\t\tceldaMorph setProperty: #punto toValue: (x@y).\r\r\t\t\t\tceldaMorph\r\t\t\t\t\tinicializarConSolucion: esNegra\r\t\t\t\t\tyEstado: estadoDelJugador.            \r\r\t\t\t\tceldaMorph alHacerClick: [ self actualizarInformacion ].\r   \t\t\t\tceldaMorph position: (xGrilla + (x * tamañoCelda)) @ (yGrilla + (y * tamañoCelda)).\r\r\t\t\t\tmainLayout addMorph: celdaMorph.\r\r\t\t\t   \tceldasMorphs add: celdaMorph.\r\r        ]\r\r    ].',			#stamp : 'Anonymous 11/19/2025 02:22',			#package : #juego-UI		}	}}OmEntry {	#tags : {		#author : 'Unknown',		#time : DateAndTime [ '2025-11-19T02:22:55.18804-03:00' ],		#prior : OmReference [ '5' ],		#self : OmReference [ '6' ]	},	#content : EpMethodModification {		#oldMethod : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #dibujarJuego,			#protocol : #drawing,			#sourceCode : 'dibujarJuego\r     | xGrilla yGrilla dimension tamañoCelda estadoDelJugador panelInfo panelInstrucciones labelInst1 labelInst2 |    \r\r   dimension := partida tablero dimension.\r\r\tpanelInfo := Morph new.\r\tpanelInfo color: Color transparent.\r\tpanelInfo layoutPolicy: TableLayout new.\r\r\tpanelInfo listDirection: #leftToRight.\r\tpanelInfo cellInset: 20.\r\r\tpanelInfo wrapCentering: #left.\r\tpanelInfo listCentering: #center.\r\r\r\tlabelVidas := Ventana new construirLabelConTexto: \'Vidas: 5\'.\r\tlabelPuntaje:= Ventana new construirLabelConTexto: \'Puntos: 0\'.\r\r\r\tpanelInfo addMorph: labelVidas.\r\tpanelInfo addMorph: labelPuntaje.\r\r\tpanelInfo position: 350 @ 10.\r\tpanelInfo width: 240.\r\tpanelInfo height: 30.\r\r\tmainLayout addMorph: panelInfo.\r\r\r\tpanelInstrucciones := Morph new.\r   panelInstrucciones color: Color transparent.\r   panelInstrucciones layoutPolicy: TableLayout new.\r\r   panelInstrucciones listDirection: #topToBottom. \r   panelInstrucciones listCentering: #right. \r   panelInstrucciones wrapCentering: #center.\r    \r\r   labelInst1 := Ventana new construirLabelConTexto: \'Clic Izq: Pintar\'.\r   labelInst2 := Ventana new construirLabelConTexto: \'Clic Der: Marcar X\'.    \r\r   panelInstrucciones addMorph: labelInst1.\r   panelInstrucciones addMorph: labelInst2.   \r\r   panelInstrucciones position: 350 @ 50.\r   panelInstrucciones width: 240.\r\tpanelInstrucciones height: 60.\r\r   mainLayout addMorph: panelInstrucciones.\r\r   dimension = 5 ifTrue: [ tamañoCelda := 30 ].\r   dimension = 10 ifTrue: [ tamañoCelda := 25 ].\r   dimension = 15 ifTrue: [ tamañoCelda := 20 ].\r\r   xGrilla := 100. \r   yGrilla := 100. \r\r\r\tself dibujarPistasFilas: yGrilla tamaño: tamañoCelda.\r   self dibujarPistasColumnas: xGrilla yGrilla: yGrilla tamaño: tamañoCelda.\r\r\r\testadoDelJugador := partida estadoJugadores first.\r\r   1 to: dimension do: [ :y |\r\r        1 to: dimension do: [ :x |\r\r            | celdaMorph esNegra |\r\r\t\t\t\tesNegra:= partida tablero esNegraEnX: x y: y.\r\t\t\t\tceldaMorph := CeldaMorph new.\r\r\t\t\t\tceldaMorph layoutPolicy: nil.\r\r\t\t\t\tceldaMorph extent: tamañoCelda @ tamañoCelda.\r\t\t\t\tceldaMorph setProperty: #punto toValue: (x@y).\r\r\t\t\t\tceldaMorph\r\t\t\t\t\tinicializarConSolucion: esNegra\r\t\t\t\t\tyEstado: estadoDelJugador.            \r\r\t\t\t\tceldaMorph alHacerClick: [ self actualizarInformacion ].\r   \t\t\t\tceldaMorph position: (xGrilla + (x * tamañoCelda)) @ (yGrilla + (y * tamañoCelda)).\r\r\t\t\t\tmainLayout addMorph: celdaMorph.\r\r\t\t\t   \tceldasMorphs add: celdaMorph.\r\r        ]\r\r    ].',			#stamp : 'Anonymous 11/19/2025 02:13',			#package : #juego-UI		},		#newMethod : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #dibujarJuego,			#protocol : #drawing,			#sourceCode : 'dibujarJuego\r    | dimension tamañoCelda estado1 estado2 anchoVentana vLabel1 pLabel1 panelInfo1 vLabel2 pLabel2 panelInfo2|\r    \r    dimension := partida tablero dimension.\r    \r    "--- CONFIGURACIÓN DE TAMAÑO ---"\r    dimension = 5 ifTrue: [ tamañoCelda := 30 ].\r    dimension = 10 ifTrue: [ tamañoCelda := 25 ].\r    dimension = 15 ifTrue: [ tamañoCelda := 20 ].\r\r    "========================================================"\r    " JUGADOR 1 (Siempre se dibuja en X=0) "\r    "========================================================"\r    estado1 := partida estadoJugadores first.\r    \r\r    vLabel1 := self construirLabelConTexto: \'Vidas: \', estado1 vidas asString.\r    pLabel1 := self construirLabelConTexto: \'Puntos: \', estado1 puntaje asString.\r    \r    panelInfo1 := Morph new color: Color transparent; layoutPolicy: TableLayout new; listDirection: #leftToRight; cellInset: 20.\r    panelInfo1 addMorph: vLabel1; addMorph: pLabel1.\r    panelInfo1 position: 350 @ 10. "Posición normal"\r    mainLayout addMorph: panelInfo1.\r    \r    "2. Dibujamos Pistas J1"\r    self dibujarPistasFilas: 100 tamaño: tamañoCelda enX: 10.\r    self dibujarPistasColumnas: 100 yGrilla: 100 tamaño: tamañoCelda.\r    \r    "3. Dibujamos Grilla J1"\r    1 to: dimension do: [ :y |\r        1 to: dimension do: [ :x |\r            | celda esNegra |\r            esNegra := partida tablero esNegraEnX: x y: y.\r            celda := CeldaMorph new layoutPolicy: nil; hResizing: #rigid; vResizing: #rigid; extent: tamañoCelda @ tamañoCelda.\r            celda setProperty: #punto toValue: (x@y).\r            \r            celda inicializarConSolucion: esNegra yEstado: estado1.\r            \r            "CONEXIÓN J1: Le pasamos SUS etiquetas"\r            celda alHacerClick: [ \r                self actualizarInfoPara: estado1 vidas: vLabel1 puntos: pLabel1 \r            ].\r            \r            celda position: (100 + (x * tamañoCelda)) @ (100 + (y * tamañoCelda)).\r            mainLayout addMorph: celda.\r            celdasMorphs add: celda.\r        ]\r    ].\r\r    "========================================================"\r    " JUGADOR 2 (Solo si existe, se dibuja en X=600) "\r    "========================================================"\r    anchoVentana := 600. "Por defecto ancho 1P"\r    \r    (partida estadoJugadores size > 1) ifTrue: [\r        estado2 := partida estadosJugadores second.\r        anchoVentana := 1200. "Agrandamos ventana"\r        \r        "1. Creamos labels LOCALES para J2"\r\r        vLabel2 := self construirLabelConTexto: \'Vidas: \', estado2 vidas asString.\r        pLabel2 := self construirLabelConTexto: \'Puntos: \', estado2 puntaje asString.\r        \r        panelInfo2 := Morph new color: Color transparent; layoutPolicy: TableLayout new; listDirection: #leftToRight; cellInset: 20.\r        panelInfo2 addMorph: vLabel2; addMorph: pLabel2.\r        panelInfo2 position: (350 + 600) @ 10. "OFFSET 600"\r        mainLayout addMorph: panelInfo2.\r        \r        "2. Dibujamos Pistas J2 (Offset 600)"\r        self dibujarPistasFilas: 100 tamaño: tamañoCelda enX: (10 + 600).\r        self dibujarPistasColumnas: (100 + 600) yGrilla: 100 tamaño: tamañoCelda.\r        \r        "3. Dibujamos Grilla J2"\r        1 to: dimension do: [ :y |\r            1 to: dimension do: [ :x |\r                | celda esNegra |\r                esNegra := partida tablero esNegraEnX: x y: y.\r                celda := CeldaMorph new layoutPolicy: nil; hResizing: #rigid; vResizing: #rigid; extent: tamañoCelda @ tamañoCelda.\r                celda setProperty: #punto toValue: (x@y).\r                \r                celda inicializarConSolucion: esNegra yEstado: estado2.\r                \r                "CONEXIÓN J2: Le pasamos SUS etiquetas"\r                celda alHacerClick: [ \r                    self actualizarInfoPara: estado2 vidas: vLabel2 puntos: pLabel2 \r                ].\r                \r                "Posición con Offset 600"\r                celda position: ((100 + 600) + (x * tamañoCelda)) @ (100 + (y * tamañoCelda)).\r                mainLayout addMorph: celda.\r                celdasMorphs add: celda.\r            ]\r        ].\r    ].\r\r    "Ajustamos ventana"\r    window extent: anchoVentana @ 600.\r    window position: 50 @ 50.',			#stamp : 'Anonymous 11/19/2025 02:22',			#package : #juego-UI		}	}}OmEntry {	#tags : {		#author : 'Unknown',		#time : DateAndTime [ '2025-11-19T02:23:29.09204-03:00' ],		#prior : OmReference [ '6' ],		#self : OmReference [ '7' ]	},	#content : EpMethodModification {		#oldMethod : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #dibujarJuego,			#protocol : #drawing,			#sourceCode : 'dibujarJuego\r    | dimension tamañoCelda estado1 estado2 anchoVentana vLabel1 pLabel1 panelInfo1 vLabel2 pLabel2 panelInfo2|\r    \r    dimension := partida tablero dimension.\r    \r    "--- CONFIGURACIÓN DE TAMAÑO ---"\r    dimension = 5 ifTrue: [ tamañoCelda := 30 ].\r    dimension = 10 ifTrue: [ tamañoCelda := 25 ].\r    dimension = 15 ifTrue: [ tamañoCelda := 20 ].\r\r    "========================================================"\r    " JUGADOR 1 (Siempre se dibuja en X=0) "\r    "========================================================"\r    estado1 := partida estadoJugadores first.\r    \r\r    vLabel1 := self construirLabelConTexto: \'Vidas: \', estado1 vidas asString.\r    pLabel1 := self construirLabelConTexto: \'Puntos: \', estado1 puntaje asString.\r    \r    panelInfo1 := Morph new color: Color transparent; layoutPolicy: TableLayout new; listDirection: #leftToRight; cellInset: 20.\r    panelInfo1 addMorph: vLabel1; addMorph: pLabel1.\r    panelInfo1 position: 350 @ 10. "Posición normal"\r    mainLayout addMorph: panelInfo1.\r    \r    "2. Dibujamos Pistas J1"\r    self dibujarPistasFilas: 100 tamaño: tamañoCelda enX: 10.\r    self dibujarPistasColumnas: 100 yGrilla: 100 tamaño: tamañoCelda.\r    \r    "3. Dibujamos Grilla J1"\r    1 to: dimension do: [ :y |\r        1 to: dimension do: [ :x |\r            | celda esNegra |\r            esNegra := partida tablero esNegraEnX: x y: y.\r            celda := CeldaMorph new layoutPolicy: nil; hResizing: #rigid; vResizing: #rigid; extent: tamañoCelda @ tamañoCelda.\r            celda setProperty: #punto toValue: (x@y).\r            \r            celda inicializarConSolucion: esNegra yEstado: estado1.\r            \r            "CONEXIÓN J1: Le pasamos SUS etiquetas"\r            celda alHacerClick: [ \r                self actualizarInfoPara: estado1 vidas: vLabel1 puntos: pLabel1 \r            ].\r            \r            celda position: (100 + (x * tamañoCelda)) @ (100 +