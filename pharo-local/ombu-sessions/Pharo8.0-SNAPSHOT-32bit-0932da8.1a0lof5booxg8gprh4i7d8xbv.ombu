OmEntry {	#tags : {		#author : 'Unknown',		#time : DateAndTime [ '2025-11-19T02:30:09.135147-03:00' ],		#prior : OmReference [ 'Pharo8.0-SNAPSHOT-32bit-0932da8.amuj7ovxlmgx1vnd9s50f2q2w', '11' ],		#self : OmReference [ '1' ]	},	#content : EpMethodModification {		#oldMethod : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #dibujarJuego,			#protocol : #drawing,			#sourceCode : 'dibujarJuego\r    | dimension tamañoCelda estado1 estado2 anchoVentana vLabel1 pLabel1 panelInfo1 vLabel2 pLabel2 panelInfo2|\r    \r    dimension := partida tablero dimension.\r    \r    "--- CONFIGURACIÓN DE TAMAÑO ---"\r    dimension = 5 ifTrue: [ tamañoCelda := 30 ].\r    dimension = 10 ifTrue: [ tamañoCelda := 25 ].\r    dimension = 15 ifTrue: [ tamañoCelda := 20 ].\r\r    "========================================================"\r    " JUGADOR 1 (Siempre se dibuja en X=0) "\r    "========================================================"\r    estado1 := partida estadoJugadores first.\r    \r\r    vLabel1 := Ventana new construirLabelConTexto: \'Vidas: \', estado1 vidas asString.\r    pLabel1 := Ventana new construirLabelConTexto: \'Puntos: \', estado1 puntaje asString.\r    \r    panelInfo1 := Morph new color: Color transparent; layoutPolicy: TableLayout new; listDirection: #leftToRight; cellInset: 20.\r    panelInfo1 addMorph: vLabel1; addMorph: pLabel1.\r    panelInfo1 position: 350 @ 10. "Posición normal"\r    mainLayout addMorph: panelInfo1.\r    \r    "2. Dibujamos Pistas J1"\r    self dibujarPistasFilas: 100 tamaño: tamañoCelda enX: 10.\r    self dibujarPistasColumnas: 100 yGrilla: 100 tamaño: tamañoCelda.\r    \r    "3. Dibujamos Grilla J1"\r    1 to: dimension do: [ :y |\r        1 to: dimension do: [ :x |\r            | celda esNegra |\r            esNegra := partida tablero esNegraEnX: x y: y.\r            celda := CeldaMorph new layoutPolicy: nil; hResizing: #rigid; vResizing: #rigid; extent: tamañoCelda @ tamañoCelda.\r            celda setProperty: #punto toValue: (x@y).\r            \r            celda inicializarConSolucion: esNegra yEstado: estado1.\r            \r            "CONEXIÓN J1: Le pasamos SUS etiquetas"\r            celda alHacerClick: [ \r                self actualizarInfoPara: estado1 vidas: vLabel1 puntos: pLabel1 \r            ].\r            \r            celda position: (100 + (x * tamañoCelda)) @ (100 + (y * tamañoCelda)).\r            mainLayout addMorph: celda.\r            celdasMorphs add: celda.\r        ]\r    ].\r\r    "========================================================"\r    " JUGADOR 2 (Solo si existe, se dibuja en X=600) "\r    "========================================================"\r    anchoVentana := 600. "Por defecto ancho 1P"\r    \r    (partida estadoJugadores size > 1) ifTrue: [\r        estado2 := partida estadoJugadores second.\r        anchoVentana := 1200. "Agrandamos ventana"\r        \r        "1. Creamos labels LOCALES para J2"\r\r        vLabel2 := Ventana new construirLabelConTexto: \'Vidas: \', estado2 vidas asString.\r        pLabel2 := Ventana new construirLabelConTexto: \'Puntos: \', estado2 puntaje asString.\r        \r        panelInfo2 := Morph new color: Color transparent; layoutPolicy: TableLayout new; listDirection: #leftToRight; cellInset: 20.\r        panelInfo2 addMorph: vLabel2; addMorph: pLabel2.\r        panelInfo2 position: (350 + 600) @ 10. "OFFSET 600"\r        mainLayout addMorph: panelInfo2.\r        \r        "2. Dibujamos Pistas J2 (Offset 600)"\r        self dibujarPistasFilas: 100 tamaño: tamañoCelda enX: (10 + 600).\r        self dibujarPistasColumnas: (100 + 600) yGrilla: 100 tamaño: tamañoCelda.\r        \r        "3. Dibujamos Grilla J2"\r        1 to: dimension do: [ :y |\r            1 to: dimension do: [ :x |\r                | celda esNegra |\r                esNegra := partida tablero esNegraEnX: x y: y.\r                celda := CeldaMorph new layoutPolicy: nil; hResizing: #rigid; vResizing: #rigid; extent: tamañoCelda @ tamañoCelda.\r                celda setProperty: #punto toValue: (x@y).\r                \r                celda inicializarConSolucion: esNegra yEstado: estado2.\r                \r                "CONEXIÓN J2: Le pasamos SUS etiquetas"\r                celda alHacerClick: [ \r                    self actualizarInfoPara: estado2 vidas: vLabel2 puntos: pLabel2 \r                ].\r                \r                "Posición con Offset 600"\r                celda position: ((100 + 600) + (x * tamañoCelda)) @ (100 + (y * tamañoCelda)).\r                mainLayout addMorph: celda.\r                celdasMorphs add: celda.\r            ]\r        ].\r    ].\r\r    "Ajustamos ventana"\r    window extent: anchoVentana @ 600.\r    window position: 50 @ 50.\r"========================================================"\r    " AJUSTE FINAL DE VENTANA "\r    "========================================================"\r    \r    "Calculamos el tamaño necesario"\r    (partida estadoJugadores size > 1) \r        ifTrue: [ \r            "Mucha anchura para 2 jugadores"\r            anchoVentana := 1300. \r        ]\r        ifFalse: [ \r            "Anchura normal para 1 jugador"\r            anchoVentana := 650. \r        ].\r\r    "Aplicamos el tamaño (700 de alto para que sobre espacio vertical)"\r    window extent: anchoVentana @ 700.\r    \r    "Centramos la ventana en la pantalla"\r    window position: (Display extent - window extent) // 2.',			#stamp : 'Anonymous 11/19/2025 02:27',			#package : #juego-UI		},		#newMethod : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #dibujarJuego,			#protocol : #drawing,			#sourceCode : 'dibujarJuego\r    | dimension tamañoCelda anchoVentana offsetJ2 |\r    \r    dimension := partida tablero dimension.\r    \r    "--- CONFIGURACIÓN ---"\r    dimension = 5 ifTrue: [ tamañoCelda := 30 ].\r    dimension = 10 ifTrue: [ tamañoCelda := 25 ].\r    dimension = 15 ifTrue: [ tamañoCelda := 20 ].\r\r    offsetJ2 := 650. "La distancia entre el J1 y el J2"\r\r    "========================================================"\r    " JUGADOR 1 (Izquierda - X base = 0) "\r    "========================================================"\r    \r    "1. Paneles de Info J1"\r    self dibujarInfoJugador: (partida estadoJugadores first) enX: 0.\r\r    "2. Pistas J1"\r    self dibujarPistasFilas: 100 tamaño: tamañoCelda enX: 10.\r    self dibujarPistasColumnas: 100 yGrilla: 100 tamaño: tamañoCelda.\r\r    "3. Grilla J1"\r    self dibujarGrillaPara: (partida estadoJugadores first) enX: 100 y: 100 tamaño: tamañoCelda.\r\r\r    "========================================================"\r    " JUGADOR 2 (Derecha - Solo si existe) "\r    "========================================================"\r    \r    anchoVentana := 650. "Ancho para 1 jugador"\r\r    (partida estadoJugadores size > 1) ifTrue: [\r        anchoVentana := 1300. "Ancho para 2 jugadores"\r        \r        "1. Paneles de Info J2 (Sumamos offsetJ2)"\r        self dibujarInfoJugador: (partida estadoJugadores second) enX: offsetJ2.\r\r        "2. Pistas J2 (Sumamos offsetJ2)"\r        self dibujarPistasFilas: 100 tamaño: tamañoCelda enX: (10 + offsetJ2).\r        self dibujarPistasColumnas: (100 + offsetJ2) yGrilla: 100 tamaño: tamañoCelda.\r\r        "3. Grilla J2 (Sumamos offsetJ2)"\r        self dibujarGrillaPara: (partida estadoJugadores second) enX: (100 + offsetJ2) y: 100 tamaño: tamañoCelda.\r        \r        "Línea divisoria (Opcional)"\r        mainLayout addMorph: (Morph new color: Color black; extent: 2@600; position: (offsetJ2 - 20) @ 0).\r    ].\r\r    "Ajuste final de ventana"\r    window extent: anchoVentana @ 650.\r    window position: 50 @ 50.',			#stamp : 'Anonymous 11/19/2025 02:30',			#package : #juego-UI		}	}}OmEntry {	#tags : {		#author : 'Unknown',		#time : DateAndTime [ '2025-11-19T02:30:35.734147-03:00' ],		#prior : OmReference [ '1' ],		#self : OmReference [ '2' ]	},	#content : EpMethodAddition {		#method : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #'dibujarInfoJugador:enX:',			#protocol : #'as yet unclassified',			#sourceCode : 'dibujarInfoJugador: unEstado enX: unX\r    | panelInfo lblVidas lblPuntos panelInstrucciones |\r\r    "--- Panel Vidas/Puntos ---"\r    panelInfo := Morph new color: Color transparent; layoutPolicy: TableLayout new; listDirection: #leftToRight; cellInset: 20.\r    \r    lblVidas := Ventana new construirLabelConTexto: \'Vidas: \', unEstado vidas asString.\r    lblPuntos := Ventana new construirLabelConTexto: \'Puntos: \', unEstado puntaje asString.\r    \r    panelInfo addMorph: lblVidas; addMorph: lblPuntos.\r    panelInfo position: (350 + unX) @ 10. \r    panelInfo width: 240; height: 30.\r    mainLayout addMorph: panelInfo.\r\r    "--- Panel Instrucciones ---"\r    panelInstrucciones := Morph new color: Color transparent; layoutPolicy: TableLayout new; listDirection: #leftToRight; cellInset: 10.\r    panelInstrucciones addMorph: (Ventana new construirLabelConTexto: \'Clic Izq: Pintar\').\r    panelInstrucciones addMorph: (Ventana new construirLabelConTexto: \'Clic Der: X\').\r    \r    panelInstrucciones position: (20 + unX) @ 10.\r    panelInstrucciones width: 300; height: 30.\r    mainLayout addMorph: panelInstrucciones.',			#stamp : 'Anonymous 11/19/2025 02:30',			#package : #juego-UI		}	}}OmEntry {	#tags : {		#author : 'Unknown',		#time : DateAndTime [ '2025-11-19T02:31:09.675147-03:00' ],		#prior : OmReference [ '2' ],		#self : OmReference [ '3' ]	},	#content : EpMethodAddition {		#method : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #'dibujarGrillaPara:enX:y:tamaño:',			#protocol : #'as yet unclassified',			#sourceCode : 'dibujarGrillaPara: unEstado enX: xBase y: yBase tamaño: tam\r    | dimension |\r    dimension := partida tablero dimension.\r    \r    1 to: dimension do: [ :y |\r        1 to: dimension do: [ :x |\r            | celdaMorph esNegra |\r            esNegra := partida tablero esNegraEnX: x y: y.\r            \r            celdaMorph := CeldaMorph new.\r            celdaMorph layoutPolicy: nil; hResizing: #rigid; vResizing: #rigid.\r            celdaMorph extent: tam @ tam.\r            celdaMorph setProperty: #punto toValue: (x@y).                \r            \r            celdaMorph inicializarConSolucion: esNegra yEstado: unEstado.\r            \r            "Importante: Actualiza la info ESTE JUGADOR (unEstado)"\r            celdaMorph alHacerClick: [ self redibujarInfo ].\r            \r            celdaMorph position: (xBase + (x * tam)) @ (yBase + (y * tam)).\r            mainLayout addMorph: celdaMorph.\r            celdasMorphs add: celdaMorph.\r        ]\r    ].',			#stamp : 'Anonymous 11/19/2025 02:31',			#package : #juego-UI		}	}}OmEntry {	#tags : {		#author : 'Unknown',		#time : DateAndTime [ '2025-11-19T02:32:04.939147-03:00' ],		#prior : OmReference [ '3' ],		#self : OmReference [ '4' ]	},	#content : EpMethodModification {		#oldMethod : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #'dibujarGrillaPara:enX:y:tamaño:',			#protocol : #'as yet unclassified',			#sourceCode : 'dibujarGrillaPara: unEstado enX: xBase y: yBase tamaño: tam\r    | dimension |\r    dimension := partida tablero dimension.\r    \r    1 to: dimension do: [ :y |\r        1 to: dimension do: [ :x |\r            | celdaMorph esNegra |\r            esNegra := partida tablero esNegraEnX: x y: y.\r            \r            celdaMorph := CeldaMorph new.\r            celdaMorph layoutPolicy: nil; hResizing: #rigid; vResizing: #rigid.\r            celdaMorph extent: tam @ tam.\r            celdaMorph setProperty: #punto toValue: (x@y).                \r            \r            celdaMorph inicializarConSolucion: esNegra yEstado: unEstado.\r            \r            "Importante: Actualiza la info ESTE JUGADOR (unEstado)"\r            celdaMorph alHacerClick: [ self redibujarInfo ].\r            \r            celdaMorph position: (xBase + (x * tam)) @ (yBase + (y * tam)).\r            mainLayout addMorph: celdaMorph.\r            celdasMorphs add: celdaMorph.\r        ]\r    ].',			#stamp : 'Anonymous 11/19/2025 02:31',			#package : #juego-UI		},		#newMethod : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #'dibujarGrillaPara:enX:y:tamaño:',			#protocol : #'as yet unclassified',			#sourceCode : 'dibujarGrillaPara: unEstado enX: xBase y: yBase tamaño: tam\r    | dimension |\r    dimension := partida tablero dimension.\r    \r    1 to: dimension do: [ :y |\r        1 to: dimension do: [ :x |\r            | celdaMorph esNegra |\r            esNegra := partida tablero esNegraEnX: x y: y.\r            \r            celdaMorph := CeldaMorph new.\r            celdaMorph layoutPolicy: nil; hResizing: #rigid; vResizing: #rigid.\r            celdaMorph extent: tam @ tam.\r            celdaMorph setProperty: #punto toValue: (x@y).                \r            \r            celdaMorph inicializarConSolucion: esNegra yEstado: unEstado.\r            \r            "Importante: Actualiza la info ESTE JUGADOR (unEstado)"\r            celdaMorph alHacerClick: [ self actualizarInformacion  ].\r            \r            celdaMorph position: (xBase + (x * tam)) @ (yBase + (y * tam)).\r            mainLayout addMorph: celdaMorph.\r            celdasMorphs add: celdaMorph.\r        ]\r    ].',			#stamp : 'Anonymous 11/19/2025 02:32',			#package : #juego-UI		}	}}OmEntry {	#tags : {		#author : 'Unknown',		#time : DateAndTime [ '2025-11-19T02:36:00.080147-03:00' ],		#prior : OmReference [ '4' ],		#self : OmReference [ '5' ]	},	#content : EpMethodModification {		#oldMethod : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #dibujarJuego,			#protocol : #drawing,			#sourceCode : 'dibujarJuego\r    | dimension tamañoCelda anchoVentana offsetJ2 |\r    \r    dimension := partida tablero dimension.\r    \r    "--- CONFIGURACIÓN ---"\r    dimension = 5 ifTrue: [ tamañoCelda := 30 ].\r    dimension = 10 ifTrue: [ tamañoCelda := 25 ].\r    dimension = 15 ifTrue: [ tamañoCelda := 20 ].\r\r    offsetJ2 := 650. "La distancia entre el J1 y el J2"\r\r    "========================================================"\r    " JUGADOR 1 (Izquierda - X base = 0) "\r    "========================================================"\r    \r    "1. Paneles de Info J1"\r    self dibujarInfoJugador: (partida estadoJugadores first) enX: 0.\r\r    "2. Pistas J1"\r    self dibujarPistasFilas: 100 tamaño: tamañoCelda enX: 10.\r    self dibujarPistasColumnas: 100 yGrilla: 100 tamaño: tamañoCelda.\r\r    "3. Grilla J1"\r    self dibujarGrillaPara: (partida estadoJugadores first) enX: 100 y: 100 tamaño: tamañoCelda.\r\r\r    "========================================================"\r    " JUGADOR 2 (Derecha - Solo si existe) "\r    "========================================================"\r    \r    anchoVentana := 650. "Ancho para 1 jugador"\r\r    (partida estadoJugadores size > 1) ifTrue: [\r        anchoVentana := 1300. "Ancho para 2 jugadores"\r        \r        "1. Paneles de Info J2 (Sumamos offsetJ2)"\r        self dibujarInfoJugador: (partida estadoJugadores second) enX: offsetJ2.\r\r        "2. Pistas J2 (Sumamos offsetJ2)"\r        self dibujarPistasFilas: 100 tamaño: tamañoCelda enX: (10 + offsetJ2).\r        self dibujarPistasColumnas: (100 + offsetJ2) yGrilla: 100 tamaño: tamañoCelda.\r\r        "3. Grilla J2 (Sumamos offsetJ2)"\r        self dibujarGrillaPara: (partida estadoJugadores second) enX: (100 + offsetJ2) y: 100 tamaño: tamañoCelda.\r        \r        "Línea divisoria (Opcional)"\r        mainLayout addMorph: (Morph new color: Color black; extent: 2@600; position: (offsetJ2 - 20) @ 0).\r    ].\r\r    "Ajuste final de ventana"\r    window extent: anchoVentana @ 650.\r    window position: 50 @ 50.',			#stamp : 'Anonymous 11/19/2025 02:30',			#package : #juego-UI		},		#newMethod : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #VentanaJuego,				#isMetaSide : false			},			#name : #dibujarJuego,			#protocol : #drawing,			#sourceCode : 'dibujarJuego\r    | dimension tamañoCelda anchoVentana offsetJ2 estado1 estado2 vLabel1 pLabel1 vLabel2 pLabel2 panelInst1 panelInfo1 panelInst2 panelInfo2|\r    \r    "1. Limpieza y Configuración Básica"\r    mainLayout delete.\r    mainLayout := Morph new color: (Color gray: 0.85).\r    mainLayout hResizing: #spaceFill; vResizing: #spaceFill.\r    window addMorph: mainLayout frame: (0@0 extent: 1@1).\r\r    dimension := partida tablero dimension.\r    \r    dimension = 5 ifTrue: [ tamañoCelda := 30 ].\r    dimension = 10 ifTrue: [ tamañoCelda := 25 ].\r    dimension = 15 ifTrue: [ tamañoCelda := 20 ].\r\r    offsetJ2 := 650. "Distancia entre J1 y J2"\r\r    "========================================================"\r    " JUGADOR 1 (Izquierda) "\r    "========================================================"\r    estado1 := partida estadoJugadores first.\r\r\r    panelInst1 := Morph new color: Color transparent; layoutPolicy: TableLayout new; listDirection: #leftToRight; cellInset: 15; listCentering: #left; wrapCentering: #center.\r    panelInst1 addMorph: (self construirLabelConTexto: \'Clic Izq: Pintar\').\r    panelInst1 addMorph: (self construirLabelConTexto: \'Clic Der: X\').\r    panelInst1 position: 20 @ 10. \r    panelInst1 width: 300; height: 30.\r    mainLayout addMorph: panelInst1.\r\r    "--- B. Puntos J1 (Arriba Derecha) ---